{
  "name": "sanity-plugin-social-embed",
  "version": "1.0.0",
  "description": "Embed social media platform post on sanity",
  "type": "commonjs",
  "sideEffects": true,
  "browserslist": "extends @sanity/browserslist-config",
  "keywords": [
    "sanity",
    "sanity-plugin"
  ],
  "homepage": "https://github.com/chitranjan-gupta/sanity-plugin-social-embed#readme",
  "bugs": {
    "url": "https://github.com/chitranjan-gupta/sanity-plugin-social-embed/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/chitranjan-gupta/sanity-plugin-social-embed.git"
  },
  "license": "MIT",
  "author": "Chitranjan Gupta <chitranjankumargupta123@gmail.com>",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "import": "./dist/index.esm.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.esm.mjs"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.esm.js",
  "source": "./src/index.ts",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "sanity.json",
    "src",
    "v2-incompatible.js"
  ],
  "scripts": {
    "build": "run-s clean && plugin-kit verify-package --silent && pkg-utils build --strict && pkg-utils --strict",
    "clean": "rimraf dist",
    "format": "prettier --write --cache --ignore-unknown .",
    "link-watch": "plugin-kit link-watch",
    "lint": "eslint .",
    "prepublishOnly": "run-s build",
    "watch": "pkg-utils watch --strict",
    "prepare": "husky install",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@sanity/incompatible-plugin": "^1.0.4",
    "jest": "^29.7.0",
    "react-player": "^2.15.1",
    "react-social-media-embed": "^2.5.9",
    "react-tweet": "^3.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@sanity/pkg-utils": "^5.1.4",
    "@sanity/plugin-kit": "^3.1.10",
    "@sanity/semantic-release-preset": "4.1.6",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/styled-components": "^5.1.34",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-sanity": "^7.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-is": "^18.2.0",
    "rimraf": "^5.0.5",
    "sanity": "^3.35.2",
    "semantic-release": "23.0.0-beta.1",
    "styled-components": "^6.1.8",
    "typescript": "5.4.2"
  },
  "peerDependencies": {
    "react": "^18",
    "sanity": "^3"
  },
  "engines": {
    "node": ">=14"
  }
}
